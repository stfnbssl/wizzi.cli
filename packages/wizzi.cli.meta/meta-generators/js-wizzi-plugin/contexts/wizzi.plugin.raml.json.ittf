{
    $
        var pluginBasename = "raml";

    $include commons
    $include v08

    usePackageVersion true

    { App
        name "RAML plugin"
        kind "WizziPlugin"

    [ Schemas
        {
            name "raml"
            plugin_type "data"
            hasNamespaces true
            hasTextContents true
            [ includes
            [ transformers
            [ artifacts
                {
                    name "document"
                    outmime "raml"

    { Utils
        useInflection true            
        useParseUrl true   
