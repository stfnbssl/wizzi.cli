module
    :import-type 
        @ ActionArgs
        @ LoaderArgs
        from "@remix-run/node"
    import 
        @ json
        from "@remix-run/node"
    import 
        @ Form
        @ useLoaderData
        from "@remix-run/react"
    import 
        @ authenticator
        @ supabaseStrategy
        from "~/auth.server"
    export 
        const action
            async=> 
                { 
                    @ request
                    :ref ActionArgs
                await 
                    _ authenticator.logout
                        @ request
                        { 
                            @ redirectTo "/"
    export 
        const loader
            async=> 
                { 
                    @ request
                    :ref LoaderArgs
                const session
                    await 
                        _ supabaseStrategy.checkSession
                            @ request
                            { 
                                @ failureRedirect "/login"
                return 
                    _ json
                        { 
                            @ email session.user?.email
    export-default 
        function Screen
            const 
                { 
                    @ email
                =
                    _ useLoaderData
                        :param 
                            :typeof loader
            return 
                < React.Fragment
                    h1 
                        + Hello
                        + {email}
                    < Form 
                        @ method "post"
                        button 
                            + Log Out
